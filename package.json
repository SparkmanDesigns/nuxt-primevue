{
  "name": "@sfxcode/nuxt-primevue",
  "version": "0.9.9",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Tom Lamers",
    "email": "tom@sfxcode.com"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:start": "node playground/.output/server/index.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0 -n changelog/config.ts"
  },
  "build": {
    "failOnWarn":false
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.2",
    "@sfxcode/formkit-primevue": ">=0.9.5",
    "@formkit/nuxt": "1.0.0-beta.14",
    "primeicons": ">=6.0.1",
    "primevue": ">=3.23.0"
  },
  "devDependencies": {
    "@anu-vue/preset-theme-default": "^0.12.0",
    "@iconify-json/mdi": "^1.1.45",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.2",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@unocss/nuxt": "^0.49.4",
    "@vueuse/nuxt": "^9.12.0",
    "anu-vue": "^0.12.0",
    "conventional-changelog-cli": "^2.2.2",
    "defu": "^6.1.2",
    "eslint": "^8.33.0",
    "flexsearch": "^0.7.31",
    "markdown-it": "^13.0.1",
    "nuxt": "^3.1.2",
    "sass": "^1.58.0",
    "unocss": "^0.49.4",
    "vitepress": "1.0.0-alpha.45",
    "vitepress-plugin-search": "1.0.4-alpha.19",
    "@types/node": "^18.11.18"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "nuxt-primevue",
    "formkit",
    "formkit-primevue"
  ]
}
